@model Veiculo

@{
    ViewData["Title"] = "Cadastrar";
}

<h1>Cadastrar</h1>

<msg mensagem="@TempData["msg"]"></msg>

<form asp-action="cadastrar">

    <partial name="_Formulario" />

    <input type="submit" value="Cadastrar" class="btn btn-outline-primary" />
    <a asp-action="Index" class="btn btn-outline-danger">Veículos</a>

</form>

